    <script>
      function showMenu(){
          var status = document.getElementById('menu').style.display;
          if (status == 'none'){
              document.getElementById('menu').style.display = 'block';
	} else {
            document.getElementById('menu').style.display = 'none';
       }
       }
       document.getElementById('menu').style.display = 'none';
    </script>
    
